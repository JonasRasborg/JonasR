<UserControl x:Class="CPRControl.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CPRControl"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="200">
    <Grid>
        <Label x:Name="label" Content="CPR" HorizontalAlignment="Left" Margin="17,7,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxCPR" HorizontalAlignment="Left" Height="23" Margin="64,10,0,0" TextWrapping="Wrap" Text="{Binding Path=CPR}" VerticalAlignment="Top" Width="120"/>

    </Grid>
</UserControl>
